{
  "name": "console-pretty",
  "version": "0.1.0",
  "description": "console.log with pretty colors",
  "main": "index.js",
  "sourceType": "module",
  "scripts": {
    "test": "jest --notify",
    "test-dev": "nodemon --exec \"jest --notify\"",
    "build": "browserify pretty.js -o ./dist/console.pretty.js"
  },
  "keywords": [
    "console",
    "log",
    "color",
    "colors"
  ],
  "author": "Oran C <oranbusiness@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wisehackermonkey/console.pretty.git"
  },
  "bugs": {
    "url": "https://github.com/wisehackermonkey/console.pretty/issues"
  },
  "homepage": "https://github.com/wisehackermonkey/console.pretty#readme",
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "preserve"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "jest": "^27.4.5",
    "node-notifier": "^10.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^1.14.3"
  }
}